/****CLASS****/

.form{
	height: 15px;
	background-color: #24190d;
	border: 1px solid #000000;
	color: #665544;
	font-family: Verdana;
	font-size: 10px;
	text-shadow: 0em 0em 0.2em #000;
}

.form{
	width: 140px;
}

.infos_compte a{
	color: #665544;
	font-family: Verdana;
	font-size: 10px;
	margin-top: 5px;
	text-shadow: 0em 0em 0.2em #000;
	text-decoration: none;
}

.infos_compte a:hover{
	color: #513d29;
	text-shadow: 0em 0em 0.2em #513d29;
	text-decoration: none
}

.connexion, .compte{
	float: left;
	margin-top: 45px;
	margin-left: 25px;
}

.connexion{
  margin-top: 80px;
}
.compte table{
	background-color: #4c351f;
  width: 285px;
}

.compte td{
  width: 142px;
	background-color: #251a0f;
}

.compte td:hover{
  width: 142px;
	background-color: #241d17;
}


.barre{
	color: #706359;
	font-size: 14px;
	font-family: Verdana;
	width: 889px;
	margin-top: 269px;
	margin-left: 44px;
