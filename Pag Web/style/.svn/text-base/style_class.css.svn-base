fieldset.error{
  border: 2px solid #c60808;
  margin: 5px 0;
  color: #c60808;
  background: #f4b3b3;
  background-image: url('../images/exclamation.png');
  background-repeat: no-repeat;
  background-position: 5px;
}

fieldset.succes{
  border: 2px solid #22a309;
  margin: 5px 0;
  color: #22a309;
  background: #c8f4b3;
  background-image: url('../images/accept.png');
  background-repeat: no-repeat;
  background-position: 5px;
}

fieldset.infos{
  border: 2px solid #368dfe;
  margin: 5px 0;
  color: #368dfe;
  background: #add0fe;
  background-image: url('../images/information.png');
  background-repeat: no-repeat;
  background-position: 5px;
}

