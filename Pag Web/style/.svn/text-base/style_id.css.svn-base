.send select{
  width: 200px;
  height: 25px;
  color: #816a50;
  padding: 3px 3px 3px 3px;
  background: url(../images/input.png) no-repeat;
  border: none;
}

.send input[type=submit]{ 
  width: 110px;
  height: 30px; 
  cursor: pointer;
  padding: 0;
  margin: 9px 0 5px 0;
  background: url(../images/envoyer.png) top no-repeat;
  border: none;
}

.send input[type=submit]:hover{ 
  background: url(../images/envoyer.png) bottom no-repeat;
}

element.style {
  vertical-align: top;
}

table.item td {
  line-height: 1.2em;
  padding: 6px;
}

.bordure1 {
  border-bottom: 1px solid #0F0200;
}

.bordure2 {
  border-bottom: 1px dashed #0F0200;
}

.bordure3 {
  border-bottom: 1px dashed #0F0200;
  border-left: 1px solid #0F0200;
  border-right: 1px solid #0F0200;
}

.bordure4 {
  border-bottom: 1px solid #0F0200;
  border-left: 1px solid #0F0200;
  border-right: 1px solid #0F0200;
}

.bordure5 {
  border-bottom: 1px solid #0F0200;
  border-right: 1px solid #0F0200;
}

table.item {
  border-collapse: collapse;
  border-spacing: 0;
  color: #000000;
}

#item_tooltip_top {
  background: url("../images/s_top.png") repeat scroll 0 0 transparent;
  height: 7px;
  width: 590px;
}

#item_tooltip_middle {
  background: url("../images/s_middle.png") repeat scroll 0 0 transparent;
  border: medium none;
  width: 590px;
}

#item_tooltip_bottom {
  background: url("../images/s_bottom.png") repeat scroll 0 0 transparent;
  height: 8px;
  width: 590px;
}

